#include<stdio.h>

int main() {   
    int n = 3;
    int matrix[n][n];

    // Input the matrix elements
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < n; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }

    // Print the transposed matrix
    for(int j = 0; j < n; j++) {
        for(int i = 0; i < n; i++) {
            printf("%d ", matrix[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}
