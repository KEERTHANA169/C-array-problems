#include<stdio.h>
#define s 10

int main()
{
    int arr[s] = {12, 7, 19, 24, 36, 45, 50, 29, 64, 81};
    
    printf("Even numbers in the array are: ");
    
    for(int i = 0; i < s; i++)
    {
        if(arr[i] % 2 == 0) // Check if the element is even
        {
            printf("%d ", arr[i]); // Print the even number
        }
    }
    
    printf("\n");
    
    return 0;
}
