#include<stdio.h>

int main() {
    int n = 3;
    int arr[] = {2, 4, 3};

    // Handle case when there is only one element
    if (n == 1) {
        printf("%d", arr[0]);
        return 0;
    }

    // Check if the first element is a peak
    if (arr[0] > arr[1]) {
        printf("%d ", arr[0]);
    }

    // Check for peaks in the middle of the array
    for (int i = 1; i < n - 1; i++) {
        if (arr[i] > arr[i - 1] && arr[i] > arr[i + 1]) {
            printf("%d ", arr[i]);
        }
    }

    // Check if the last element is a peak
    if (arr[n - 1] > arr[n - 2]) {
        printf("%d", arr[n - 1]);
    }

    return 0;
}
