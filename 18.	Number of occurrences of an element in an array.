#include<stdio.h>
#define s 10

int main() {
    int arr[s] = {2, 2, 2, 2, 2, 5, 6, 5, 8, 1};
    int count[10] = {0};
    
    // Count occurrences of each element
    for (int i = 0; i < s; i++) {
        count[arr[i]]++;
    }
    
    int num = 1;  // Element to find occurrences of
    int ans = count[num];
    
    printf("The element %d appears %d times in the array.\n", num, ans);
    
    return 0;
}
