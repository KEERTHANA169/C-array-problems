#include<stdio.h>
int main()
{   
    int matrix[5][5];
       for(int i=0;i<5;i++)
       {
           for(int j=0;j<5;j++)
           {
               scanf("%d",&matrix[i][j]);
           }
       }
       int sum=0;
       for(int i=0;i<5;i++)
       {
           sum+=matrix[i][i];
       }
       printf("%d",sum);
        return 0;
}

