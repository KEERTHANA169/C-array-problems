#include<stdio.h>
#define s 10

int main()
{
    int arr[s] = {1, 2, 3, 4, 2, 5, 6, 7, 9, 10};
    int sum = 0;
    
    for(int i = 0; i < s; i++)
    {
        if(arr[i] % 2 == 1) // Check if the element is odd
        {
            sum += arr[i]; // Add to sum if it is odd
        }
    }
    
    printf("Sum of odd elements: %d\n", sum);
    
    return 0;
}
